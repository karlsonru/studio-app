    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.6.1",
    "mongoose": "^6.6.1",
    "react": "^18.2.0"
  }
