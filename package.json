{
  "type": "module",
  "name": "studio-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config --experimental-specifier-resolution=node .",
    "prestart": "npm run build",
    "server": "nodemon -r dotenv/config --experimental-specifier-resolution=node backend/app.ts",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint:fix": "npx eslint --cache --ext .tsx --ext .ts . --fix",
    "lint": "npx eslint --cache --ext .tsx --ext .ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlsonru/studio-app.git"
  },
